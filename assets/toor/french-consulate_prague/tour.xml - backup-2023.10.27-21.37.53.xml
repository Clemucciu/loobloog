	<scene name="scene_CourExt" title="CourExt" onstart="" thumburl="panos/CourExt.tiles/thumb.jpg" lat="" lng="" alt="" heading="">
		
		<control bouncinglimits="calc:image.cube ? true : false" />

		<view hlookat="0.0" vlookat="0.0" fovtype="MFOV" fov="120" maxpixelzoom="2.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="panos/CourExt.tiles/preview.jpg" />

		<image>
			<cube url="panos/CourExt.tiles/%s/l%l/%v/l%l_%s_%v_%h.jpg" multires="512,1024,2048" />
		</image>

	</scene>

